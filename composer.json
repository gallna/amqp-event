{
    "name": "gallna/amqp-event",
    "description": "Amqp message dispatcher",
    "require": {
        "symfony/event-dispatcher": "~2.6",
        "php": ">=5.4.0",
        "ext-amqp": "*"
    },
    "require-dev": {
        "monolog/monolog": "~1.2"
    },
    "suggest": {
        "monolog/monolog": "Allow sending log messages to a cli console"
    },
    "authors": [
        {
            "name": "Tomasz Jonik",
            "email": "tom.jonik@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kemer\\Amqp\\Addons\\": "src/Kemer/Addons",
            "Kemer\\Amqp\\": "src/Kemer/Amqp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "examples/app"
        }
    }
}
